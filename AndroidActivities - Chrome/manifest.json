  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "homepage_url": "http://activities.droidicon.com/",
  "permissions": [
    "https://play.google.com/",
    "http://activities.droidicon.com/"
  ]
}
