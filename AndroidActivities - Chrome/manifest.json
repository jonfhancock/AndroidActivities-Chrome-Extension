{
  "name": "Android Activities",
  "version": "1.1",
  "description": "Get the Activity names for Android apps when browsing the Android Market.",

    "content_scripts": [
    {
      "matches": ["https://play.google.com/*"],
      "js": ["jquery.min.js", "content.js"]
    }
